<?xml version="1.0" encoding="utf-8"?>
    <com.tencent.libpag.sample.libpag_sample.FoldSlidingPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/id_drawerLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <fragment
            android:id="@+id/left_menu"
            android:name="com.tencent.libpag.sample.libpag_sample.LeftMenuFragment"
            android:layout_width="240dp"
            android:layout_height="match_parent"/>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/timg"/>
        </RelativeLayout>
    </com.tencent.libpag.sample.libpag_sample.FoldSlidingPanelLayout>
